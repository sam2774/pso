
public class GenBot 
{
	//private double xCoord;
	//private double yCoord;
	private int numberOfCoords = 2;
	private double acceleration = 2.0;
	private double[] velocity = new double[numberOfCoords];
	private double[] coords = new double[numberOfCoords];
	private double[] pBest = new double [numberOfCoords];
	
	/*public double GetXCoord()
	{
		return xCoord;
	}
	
	public double GetYCoord()
	{
		return yCoord;
	}
	
	public void SetXCoord(double newX)
	{
		xCoord = newX;
	}
	
	public void SetYCoord(double newY)
	{
		yCoord = newY;
	}*/
	
	public double[] getVel(){
		return velocity;
	}
	
	public double[] getX(){
		return coords;
	}
	
	public double[] getP(){
		return pBest;
	}
	
	public void setVel(double foo, double bar){
		velocity[0] = foo;
		velocity[1] = bar;
	}
	
	public void setX(double foo, double bar){
		coords[0] = foo;
		coords[1] = bar;
	}
	
	public void setP(double foo, double bar){
		pBest[0] = foo;
		pBest[1] = bar;
	}
}
