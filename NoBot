
public class NoBot 
{
	private int xCoord;
	private int yCoord;
	
	public int getX()
	{
		return xCoord;
	}
	
	public int getY()
	{
		return yCoord;
	}
	
	public void up()
	{
		yCoord = yCoord + 1;
	}
	
	public void down()
	{
		yCoord = yCoord - 1;
	}
	
	public void left()
	{
		yCoord = yCoord - 1;
	}
	
	public void right()
	{
		xCoord = xCoord + 1;
	}
}
